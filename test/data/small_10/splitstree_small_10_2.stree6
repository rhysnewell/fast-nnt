BEGIN ALGORITHM;
TITLE 'Show Splits';
LINK SPLITS = 'Splits';
NAME 'Show Splits';
OPTIONS
	View = 'SplitNetwork';
END; [ALGORITHM]

BEGIN VIEW;
TITLE 'SplitNetwork';
LINK ALGORITHM = 'Show Splits';
NAME 'SplitNetwork';
INPUT 'SPLITS';
OPTIONS
	Diagram = 'Splits',
	Orientation = 'Rotate0Deg',
	Rooting = 'None',
	ZoomFactor = 1,
	FontScaleFactor = 1,
	RootAngle = 160,
	OutlineFill = 0xa9a9a9ff,
	LabelSplitsBy = 'None',
	ShowQRCode = 'false';
END; [VIEW]
